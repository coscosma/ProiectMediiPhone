<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="ProiectMediiPhone.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:ProiectMediiPhone"
       Shell.FlyoutBehavior="Flyout"
       Shell.TabBarIsVisible="True">

    <Tab Title="Home">
        <ShellContent ContentTemplate="{DataTemplate local:LoginPage}" />
    </Tab>

    <Tab Title="Alege Masina">
        <ShellContent ContentTemplate="{DataTemplate local:MasiniListPage}" Route="MainPage" />
    </Tab>

    <Tab Title="Delogheaza-te!">
        <ShellContent ContentTemplate="{DataTemplate local:LoginPage}" />
    </Tab>

</Shell>
